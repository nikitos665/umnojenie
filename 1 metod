import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner p = new Scanner(System.in);
        int a; int b; int s = 0;
        System.out.println("Введите а = ");
        a = p.nextInt();
        System.out.println("Введите b = ");
        b = p.nextInt();
        for (int i = 1; i <=b; i++){
            s +=a;
        }
        System.out.println("Ответ = "+s);
    }
